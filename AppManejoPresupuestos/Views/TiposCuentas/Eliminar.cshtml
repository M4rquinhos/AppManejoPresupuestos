@model TipoCuenta

@{
    ViewData["Title"] = "Eliminar Tipo Cuenta";
}


<h3 class="mb-3">¿Esta seguro de que quieres borrar el tipo cuenta @Model.Nombre?</h3>

<form asp-action="EliminarTipoCuenta">
    <input type="hidden" asp-for="IdTipoCuenta">
    <button type="submit" class="btn btn-danger">Borrar</button>
    <a class="btn btn-secondary" asp-action="Index">Cancelar</a>
</form>